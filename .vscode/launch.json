    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Super",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "buildFlags": "-tags 'novpp'",
            "env": {"CGO_CFLAGS":"-I/usr/include/memif"},
            "args":["-config","example_config/super_mode/EgNet_super.yaml","-mode","super"/*,"-example"*/],
        },
        {
            "name": "Launch Edge",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "buildFlags": "-tags 'novpp'",
            "env": {"CGO_CFLAGS":"-I/usr/include/memif"},
            "args":["-config","example_config/p2p_mode/EgNet_edge1.yaml","-mode","edge"/*,"-example"*/],
        },
        {
            "name": "Launch GenCfg",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "buildFlags": "-tags 'novpp'",
            "env": {"CGO_CFLAGS":"-I/usr/include/memif"},
            "args":["-config","example_config/super_mode/gensuper.yaml","-mode","gencfg","-cfgmode","super"/*,"-example"*/],
        }
    ]
}